{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "headers": [ {
      "source": "**/*.@(jpg|jpeg|gif|png|css|js)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000"
      } ]
    } ]
  }
}
